{
  "name": "FlowiseAI MCP",
  "description": "Comprehensive MCP server for FlowiseAI with full API coverage including chatflows, assistants, AgentFlow V2, RAG operations, and streaming support",
  "author": "FlowiseAI MCP",
  "version": "1.0.0",
  "license": "MIT",
  "repository": "https://github.com/MilesP46/FlowiseAI-MCP",
  "homepage": "https://github.com/MilesP46/FlowiseAI-MCP#readme",
  "keywords": [
    "flowiseai",
    "mcp",
    "ai",
    "chatflow",
    "agentflow",
    "rag",
    "automation",
    "streaming",
    "llm"
  ],
  "categories": [
    "AI & Machine Learning",
    "Automation",
    "Developer Tools"
  ],
  "runtime": "python",
  "command": "uvx",
  "args": [
    "--from",
    "git+https://github.com/MilesP46/FlowiseAI-MCP.git",
    "flowiseai-mcp"
  ],
  "env": {
    "FLOWISEAI_URL": {
      "description": "FlowiseAI instance URL (localhost, network, or cloud)",
      "required": true,
      "default": "http://localhost:3000"
    },
    "FLOWISEAI_API_KEY": {
      "description": "API key for FlowiseAI authentication",
      "required": true,
      "sensitive": true
    }
  },
  "capabilities": {
    "tools": true,
    "resources": true,
    "streaming": true,
    "async": true
  },
  "features": [
    "Complete FlowiseAI API coverage",
    "Streaming predictions with SSE",
    "AgentFlow V2 with form support",
    "Document Store and RAG operations",
    "Human-in-the-loop support",
    "Session and memory management",
    "File upload handling",
    "Dynamic configuration",
    "Multi-agent support"
  ],
  "requirements": {
    "python": ">=3.10",
    "flowiseai": ">=1.0.0"
  },
  "documentation": {
    "readme": "README.md",
    "api": "https://docs.flowiseai.com/api-reference",
    "examples": "README.md#example-usage"
  }
}